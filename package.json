  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start": "npm run recreate:database && npm run create:admin && babel-node ./server/app.js",
    "build": "babel ./server/ -d ./server/dist",
    "heroku-postbuild": "npm run build",
    "start:dev": "nodemon --exec babel-node ./server/app.js ",
    "recreate:database": "babel-node ./server/db/runScript.js",
    "create:admin": "babel-node ./server/db/createAnAdmin.js",
    "test": "set NODE_ENV=test&& npm run recreate:database && npm run create:admin && nyc --reporter=html --reporter=text mocha --require @babel/register --require babel-polyfill --timeout 10000 ./server/tests/*.js --exit"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "randomstring": "^1.1.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-plugin-import": "^2.15.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0"
  }
}
